      <command Description="Store the current theta angle to temp var m_1mlTipAngle" ServerCommand="Subsystem: Calibration calibrate1mlTipAngle;" IsManualStep="false" IsMessageOnly="false" IsBackStop="false" AxisCoarse="0" AxisFine="0" AxisMin="0" AxisMax="0" />
      <command Description="Move the theta axis using the 'Manual Axis Controls' so that robot tip is centred directly above the #5 radius etch line on the calibration jig. You may move the carousel by hand to help confirm the alignment." IsManualStep="true" IsMessageOnly="false" IsBackStop="true" AxisCoarse="0" AxisFine="0" AxisMin="0" AxisMax="0" />
