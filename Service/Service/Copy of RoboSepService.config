    <script name="Carousel">
      <command Description="Locate the calibration jig on quadrant 1 of the carousel. Ensure that the theta axis is calibrated before commencing calibration of the carousel." IsManualStep="false" IsMessageOnly="true" IsBackStop="true" AxisCoarse="0" AxisFine="0" AxisMin="0" AxisMax="0" />
      <command Description="Enter calibration mode." ServerCommand="Subsystem: Calibration startCalibration;" IsManualStep="false" IsMessageOnly="false" IsBackStop="false" AxisCoarse="0" AxisFine="0" AxisMin="0" AxisMax="0" />
      <command Description="Home all axes." ServerCommand="Subsystem: Instrument HomeAll 0;" IsManualStep="false" IsMessageOnly="false" IsBackStop="false" AxisCoarse="0" AxisFine="0" AxisMin="0" AxisMax="0" />
      <command Description="Locate the robot tip head above the carousel calibration slot." ServerCommand="Subsystem: TeslaPlatform MoveTo 1 'calibrationjigslot_degrees' 135;" IsManualStep="false" IsMessageOnly="false" IsBackStop="true" AxisCoarse="0" AxisFine="0" AxisMin="0" AxisMax="0" />
      <command Description="Set Robot_ZAxis defaults" IsManualStep="false" IsMessageOnly="false" IsBackStop="false" AxisName="Robot_ZAxis" AxisCoarse="5" AxisFine="1" AxisMin="0" AxisMax="0" AxisUnits="mm" />
      <command Description="Set Robot_ThetaAxis defaults" IsManualStep="false" IsMessageOnly="false" IsBackStop="false" AxisName="Robot_ThetaAxis" AxisCoarse="2.5" AxisFine="0.5" AxisMin="0" AxisMax="0" AxisUnits="deg" />
      <command Description="Set Carousel defaults" IsManualStep="false" IsMessageOnly="false" IsBackStop="false" AxisName="Carousel" AxisCoarse="2.5" AxisFine="0.25" AxisMin="0" AxisMax="0" AxisUnits="deg" />
      
      <command Description="Lower the tip-end of the tiphead so it is just inside of the slot. Move carousel clockwise(CW) until the Nozzle tip-end kisses the edge of side ‘A’. Then press Accept Changes." IsManualStep="true" IsMessageOnly="false" IsBackStop="false" AxisCoarse="0" AxisFine="0" AxisMin="0" AxisMax="0" />
   
      <command Description="Store the current carousel angle to temp var m_jigCarouselAngle1" ServerCommand="Subsystem: Calibration calibrateCarouselAngle1;" IsManualStep="false" IsMessageOnly="false" IsBackStop="false" AxisCoarse="0" AxisFine="0" AxisMin="0" AxisMax="0" />
      <command Description="Move the carousel to the other end of the calibration slot." ServerCommand="Device: Carousel IncrementTheta 18;" IsManualStep="false" IsMessageOnly="false" IsBackStop="true" AxisCoarse="0" AxisFine="0" AxisMin="0" AxisMax="0" />
      <command Description="Move carousel CCW until tip end touches the edge ‘B’. Then press Accept Changes." IsManualStep="true" IsMessageOnly="false" IsBackStop="false" AxisCoarse="0" AxisFine="0" AxisMin="0" AxisMax="0" />
      <command Description="Store the current carousel angle to temp var m_jigCarouselAngle2" ServerCommand="Subsystem: Calibration calibrateCarouselAngle2;" IsManualStep="false" IsMessageOnly="false" IsBackStop="false" AxisCoarse="0" AxisFine="0" AxisMin="0" AxisMax="0" />
      <command Description="Exit calibration mode. NOTE: This will save the new calibration settings to file. The new calibration will not take effect until the instrument is restarted." ServerCommand="Subsystem: Calibration endCalibration;" IsManualStep="false" IsMessageOnly="false" IsBackStop="false" AxisCoarse="0" AxisFine="0" AxisMin="0" AxisMax="0" />
      <command Description="Reset Instrument" ServerCommand="Centre reInitInstrument;" IsManualStep="false" IsMessageOnly="false" IsBackStop="false" AxisCoarse="0" AxisFine="0" AxisMin="0" AxisMax="0" />
      <command Description="Home all axes." ServerCommand="Subsystem: Instrument HomeAll 0;" IsManualStep="false" IsMessageOnly="false" IsBackStop="false" AxisCoarse="0" AxisFine="0" AxisMin="0" AxisMax="0" />
    </script>
